            <chart createdWith="OMNeT++ 6.1.0.241008-f7568267cd" icon="icons/full/obj/linechart.png" id="513113320" name="out of sync time" resultTypes="vector" template="linechart_mpl" type="MATPLOTLIB">
                <script><![CDATA[import math
from omnetpp.scave import results, chart, utils
import matplotlib.pyplot as plt
import numpy as np
from cmath import isnan
from builtins import list
from natsort import natsorted

# get chart properties
props = chart.get_properties()
utils.preconfigure_plot(props)

# collect parameters for query
filter_expression = props["filter"]
start_time = float(props["vector_start_time"] or -math.inf)
end_time = float(props["vector_end_time"] or math.inf)
omit_empty_vectors = props["omit_empty_vectors"] == "true"

# query vector data into a data frame
try:
    df = results.get_vectors(filter_expression, include_attrs=True, include_runattrs=True, include_itervars=True, start_time=start_time, end_time=end_time, omit_empty_vectors=omit_empty_vectors)
except results.ResultQueryError as e:
    raise chart.ChartScriptError("Error while querying results: " + str(e))

if df.empty:
    raise chart.ChartScriptError("The result filter returned no data.")

# apply vector operations
df = utils.perform_vector_ops(df, props["vector_operations"])

# plot
utils.add_legend_labels(df, props)
utils.sort_rows_by_legend(df, props)
# utils.plot_vectors(df, props, sort=False)

print(df.columns.values.tolist())
df['configname'] = df['configname'].str.replace("_Repeat", "").str.replace("Standby1", " Standby").str.replace("BMCA", "BTCA")
df['iterationvars'] = df['iterationvars'].str.replace("$sendImmediately=", "").str.replace("false, ","2").str.replace("true, ", "").str.replace("$numHops=", " (") + ")"
df['fullname'] = df['configname'] + df['iterationvars']

df['lastvectime'] = df.apply(
    lambda row: row['vectime'][-1] - 60.1 if row['vecvalue'][-1] == 1 else 60,
    axis=1
)

df.set_index("fullname")


grouped = df.groupby('fullname')['lastvectime'].apply(list)
print(grouped)
grouped = grouped.reindex(index=sorted(grouped.index))
grouped.to_csv("out_of_sync.csv", sep='\t')

for name, values in grouped.items():
    if isinstance(values, (int, float)) and isnan(values):
        continue

    print (values)
    #print(values)
    # Sort values for CDF
    sorted_values = np.sort(values)
    # Compute the CDF
    cdf = np.arange(1, len(sorted_values) + 1) / len(sorted_values)
    # Plot the CDF
    plt.plot(sorted_values, cdf, label=f'{name}')

utils.postconfigure_plot(props)

ax = plt.gca()
ax.set_xlim([0, 60])


#print(df['absvalue'])
#grouped = df.groupby('fullname')['lastvectime']
#print(grouped.apply(list))

#labels = []
#data = []
#for name, group in grouped:
#    labels.append(name)
#    data.append(group.values)

#plt.boxplot(data, labels=labels)


utils.export_image_if_needed(props)
utils.export_data_if_needed(df, props)
]]></script>

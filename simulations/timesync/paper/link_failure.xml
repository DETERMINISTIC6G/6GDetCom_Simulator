<scenario>
    <!-- nwtt <-> dstt breaks -->
    <set-channel-param t="70s" src-module="detCom.dstt[0]" dest-module="detCom.nwtt" par="disabled" value="true"/>
    <set-channel-param t="70s" src-module="detCom.dstt[1]" dest-module="detCom.nwtt" par="disabled" value="true"/>
    <set-channel-param t="110s" src-module="detCom.dstt[0]" dest-module="detCom.nwtt" par="disabled" value="false"/>
    <set-channel-param t="110s" src-module="detCom.dstt[1]" dest-module="detCom.nwtt" par="disabled" value="false"/>
    
    <!-- dstt1 becomes disconnected -->
    <set-channel-param t="130s" src-module="detCom.dstt[0]" dest-module="detCom.nwtt" par="disabled" value="true"/>
    <set-channel-param t="130s" src-module="detCom.dstt[0]" dest-module="detCom.dstt[1]" par="disabled" value="true"/>
    <set-channel-param t="170s" src-module="detCom.dstt[0]" dest-module="detCom.nwtt" par="disabled" value="false"/>
    <set-channel-param t="170s" src-module="detCom.dstt[0]" dest-module="detCom.dstt[1]" par="disabled" value="false"/>
</scenario>

<?xml version='1.0' encoding='UTF-8'?>
<script>

	<at t="20">
		<set-param module="device2.app[0].source"
			par="pendingEnabled" value="false" />			<!--stop flow1, dest: wirelessdevice2[0]  -->
	</at>

	<at t="40">
		<set-param module="device2.app[1].source"
			par="pendingProductionInterval" value="300ms" />			<!--run flow2, dest: wirelessdevice2[1]-->
		<set-param module="device2.app[1].source" par="maxLatency"
			value="300ms" />	
		<set-param module="device2.app[1].source"
			par="pendingPacketLength" value="1200B" />
	</at>


	<set-param t="50" module="detComOne.dstt[1]"                       
            par="delayDownlink" 
            expr="rngProvider(&quot;histogramContainer&quot;,&quot;Downlink_improve&quot;)"/>      <!-- improves -->
            
    <set-param t="50" module="detComOne.dstt[0]" 
            par="delayUplink" 
            expr="rngProvider(&quot;histogramContainer&quot;,&quot;Uplink_worse&quot;)"/>         <!-- getting worse -->

    
    <at t="60">
        <set-param module="device3.app[0].source"
            par="pendingEnabled" value="true" />			<!--run flow3, dest: wirelessdevice1[1]-->
        <set-param module="device2.app[1].source"
            par="enabled" value="false" />                  <!--stop flow2, stop immediately! -->
    </at>

    

    <set-param t="80" module="detComOne.dstt[0]" 
            par="delayDownlink" 
            expr="rngProvider(&quot;histogramContainer&quot;,&quot;Downlink_worse&quot;)"/> 
		
</script>